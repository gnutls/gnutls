msgstr "PKCS #11 PIN uzamknut"

#: lib/gnutls_errors.c:332
msgid "PKCS #11 error in session"
msgstr "PKCS #11 chyba v relaci"

#: lib/gnutls_errors.c:334
msgid "PKCS #11 error in signature"
msgstr "PKCS #11 chyba v podpisu"

